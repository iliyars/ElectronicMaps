                    xmlns:converters="clr-namespace:ElectronicMaps.WPF.Shared.Converters">
    
    <!-- Converters -->
    <converters:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
    <converters:EnumEqualsToVisibilityConverter x:Key="EnumEqualsToVisibilityConverter" />
    <converters:IndexToNumberConverter x:Key="IndexToNumberConverter" />
